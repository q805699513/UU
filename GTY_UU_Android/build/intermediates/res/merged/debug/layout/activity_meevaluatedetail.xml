<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <ImageView 
            android:id="@+id/activity_me_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/me_evaluate"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="12dp"
            />
        <TextView 
            android:id="@+id/activity_me_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="奥巴马"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="12dp"
            android:textSize="17sp"
            android:textColor="#565a5c"
            android:layout_toRightOf="@+id/activity_me_image"
            />
        
        <TextView 
            android:id="@+id/activity_me_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-08-15"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="12dp"
            android:textSize="16sp"
            android:textColor="#98999a"
            android:layout_toRightOf="@+id/activity_me_image"
            android:layout_below="@+id/activity_me_name"
            />
        <RatingBar
        android:id="@+id/activity_me_ratingbar"
        style="@style/roomRatingBarTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isIndicator="true"
        android:numStars="5"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/activity_me_image"
        android:layout_below="@+id/activity_me_data"
        android:rating="5" >
    </RatingBar>
    <TextView 
            android:id="@+id/activity_me_txtcontent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马奥巴马"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="12dp"
            android:textSize="17sp"
            android:textColor="#565a5c"
            android:layout_toRightOf="@+id/activity_me_image"
            android:layout_below="@+id/activity_me_ratingbar"
            />
    
    
    </RelativeLayout>
    <RelativeLayout 
         android:layout_width="match_parent"
         android:layout_height="86dp"
         android:layout_marginLeft="10dp"
         android:layout_marginRight="10dp"
         android:background="#f5f5f5"
         >     
     <ImageView 
         android:id="@+id/uu_user_img"
         android:layout_width="120dp"
         android:layout_marginTop="8dp"
         android:layout_marginLeft="8dp"
         android:layout_marginBottom="8dp"
         android:layout_height="wrap_content"
         />
    <TextView 
        android:id="@+id/uu_name_id"
        android:text="小U"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        android:textColor="#565a5c"
        android:layout_toRightOf="@+id/uu_user_img"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="12dp"
        />
     <TextView 
        android:id="@+id/uuuser_name_id"
        android:text="崽崽"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        android:textColor="#565a5c"
        android:layout_toRightOf="@+id/uu_name_id"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="12dp"
        />
     <TextView 
        android:id="@+id/uuuser_cityname"
        android:text="北京"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        android:textColor="#565a5c"
        android:layout_alignParentRight="true"
        android:layout_marginTop="8dp"
        android:layout_marginRight="12dp"
        />
     
     <ImageView 
         android:id="@+id/uu_constellation"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:src="@drawable/order_guide_woman"
         android:layout_toRightOf="@+id/uu_user_img"
         android:layout_below="@+id/uu_name_id"
         android:layout_marginLeft="12dp"
         android:layout_marginTop="4dp"
         />
     <TextView 
        android:id="@+id/uu_username_constellation"
        android:text="处女座"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:textColor="#98999a"
        android:layout_toRightOf="@+id/uu_constellation"
        android:layout_below="@+id/uu_name_id"
        android:layout_marginLeft="5dp"
        />
     <TextView 
        android:id="@+id/uu_username_years"
        android:text="90后"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:textColor="#98999a"
        android:layout_toRightOf="@+id/uu_username_constellation"
        android:layout_below="@+id/uu_name_id"
        android:layout_marginLeft="5dp"
        />
     
     <TextView 
        android:id="@+id/uu_username_Professional"
        android:text="旅游管理"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:textColor="#98999a"
        android:layout_toRightOf="@+id/uu_username_years"
        android:layout_below="@+id/uu_name_id"
        android:layout_marginLeft="5dp"
        />
     <ImageView 
         android:id="@+id/uu_select_img_one"
         android:layout_width="20dp"
         android:layout_height="20dp"
         android:src="@drawable/ic_launcher"
         android:layout_toRightOf="@+id/uu_user_img"
         android:layout_below="@+id/uu_constellation"
         android:layout_marginLeft="12dp"
         android:layout_marginTop="1dp"
         />
     <ImageView 
         android:id="@+id/uu_select_img_two"
         android:layout_width="20dp"
         android:layout_height="20dp"
         android:src="@drawable/ic_launcher"
         android:layout_toRightOf="@+id/uu_select_img_one"
         android:layout_below="@+id/uu_constellation"
         android:layout_marginLeft="5dp"
         android:layout_marginTop="1dp"
         />
     <ImageView 
         android:id="@+id/uu_select_img_three"
         android:layout_width="20dp"
         android:layout_height="20dp"
         android:src="@drawable/ic_launcher"
         android:layout_toRightOf="@+id/uu_select_img_two"
         android:layout_below="@+id/uu_constellation"
         android:layout_marginLeft="5dp"
         android:layout_marginTop="1dp"
         />
     <ImageView 
         android:id="@+id/uu_select_img_four"
         android:layout_width="20dp"
         android:layout_height="20dp"
         android:src="@drawable/ic_launcher"
         android:layout_toRightOf="@+id/uu_select_img_three"
         android:layout_below="@+id/uu_constellation"
         android:layout_marginLeft="5dp"
         android:layout_marginTop="1dp"
         />
     <ImageView 
         android:id="@+id/uu_select_img_five"
         android:layout_width="20dp"
         android:layout_height="20dp"
         android:src="@drawable/ic_launcher"
         android:layout_toRightOf="@+id/uu_select_img_four"
         android:layout_below="@+id/uu_constellation"
         android:layout_marginLeft="5dp"
         android:layout_marginTop="1dp"
         />
     
     </RelativeLayout>
</LinearLayout>