<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >
    <ScrollView 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        >
	<LinearLayout 
	    android:layout_width="match_parent"
   		android:layout_height="match_parent"
    	android:background="#f5f5f5"
    	android:orientation="vertical"
	    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/right_view_withdrawcash_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:src="@drawable/phone_back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_centerVertical="true"
            android:text="提现"
            android:textColor="@color/black"
            android:textSize="18sp" />

		<TextView
			android:id="@+id/withdraw_detail"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:layout_marginRight="13dp"
			android:text="提现说明"
			android:textColor="@color/black"
			android:textSize="15sp" />
    </RelativeLayout>
	<RelativeLayout
	    android:background="#ffffff"
        android:id="@+id/can_money"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:paddingRight="12dp"
        android:paddingLeft="12dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
			android:textSize="15sp"
            android:text="可提现金额"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/with_Draw_Money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="￥800"
            android:textSize="20sp"
            android:textColor="#00a1d9" />
    </RelativeLayout>
	
    <RelativeLayout
        android:background="#ffffff"
        android:id="@+id/widthdraw_cash_way_rel"
		android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp" >

        <TextView
            android:id="@+id/widthdraw_cash_bank"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="支付方式"
            android:textColor="#565a5c" />

        <ImageView
            android:id="@+id/wechat_price_monery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/person_compile_enter"
			/>

        <TextView
            android:id="@+id/widthdraw_cash_card_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/wechat_price_monery"
            android:text="添加支付方式" />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:background="#e5e5e5" />

    <RelativeLayout
        android:background="#ffffff"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp" >

        <TextView
            android:id="@+id/right_txt_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="提现金额"
            android:textColor="#000000" />

        <EditText
            android:id="@+id/recharge_price_detail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/right_txt_name"
            android:background="@null"
            android:numeric="decimal"  
            android:gravity="center_vertical|right"
            android:textSize="12sp"
            android:maxLines="1"
            android:maxLength="8" 
            android:textColor="#666666"
            android:textColorHint="#98999a"
            android:layout_marginRight="20dp"
            />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="12dp"
        android:background="#e5e5e5" />

    <Button
        android:id="@+id/Withdrawcash_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/wallet_commit_bg_shape"
        android:text="确定"
        android:textColor="@drawable/wallet_commit_bg_selector"
        android:enabled="false" />
	<TextView 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="提示："
	    android:textSize="14sp"
	    android:textColor="#00a1d9"
	    android:layout_marginLeft="12dp"
	    android:layout_marginTop="24dp"
	    />
	<TextView 
	    android:layout_marginBottom="20dp"
	    android:layout_marginLeft="12dp"
	    android:layout_marginRight="12dp"
	    android:layout_marginTop="14dp"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:textColor="#98999a"
	    android:textSize="12sp"
	    android:lineSpacingExtra="5dp"
	    android:text="您的可提现金额可能会小于您的钱包零钱。\n原因如下：\n\n1. 未完成的服务的订单，收入不可提现；\n2. u客发起退款申请的订单，收入不可提现（u客取消退款， 或小u同意退款才能结算并提现）；\n3. 已完成订单，T+1日可发起提现；\n4. 红包收入，T+0可发起提现；\n5. 退款收入，T+0可发起提现。\n\n发起提现后，您在uu客钱包账户绑定的银行卡，将会在三个工作日内收到您提现的款项。如您需要人工帮助请致电400-600-8669转5
	    "
	    />
</LinearLayout>
</ScrollView>
</LinearLayout>
